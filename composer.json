{
    "name": "a-h-abid/docker-php-mysqldump",
    "type": "project",
    "description": "The Laravel Framework.",
    "require": {
        "ifsnop/mysqldump-php": "2.9"
    },
    "require-dev": {
        "vlucas/phpdotenv": "5.3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
